{
  "functions": {
    "api/**/*.php": {
      "runtime": "vercel-php@0.7.2"
    }
  },
  "rewrites": [
    { "source": "styles/style.css", "destination": "/api/styles/style.css" },
    { "source": "/", "destination": "/api/index.php" },
    { "source": "/:path*", "destination": "/api/:path*" }
  ]
}
